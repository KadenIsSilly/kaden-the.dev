{
  "buildCommand": "npm run build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/pages/home.html"
    },
    {
      "source": "/about",
      "destination": "/pages/home.html/#about-me"
    },
    {
      "source": "/projects",
      "destination": "/pages/projects.html"
    },
    {
      "source": "/contact",
      "destination": "/pages/contact.html"
    }
  ]
}